{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "semicolon": false,
    "trailing-comma": false,
    "quotemark": false,
    "max-line-length": false,
    "align": false,
    "no-unused-expression": false,
    "no-shadowed-variable": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-ordering": [
      false
    ],
    "max-classes-per-file": false,
    "no-reference": false,
    "no-namespace": [
      false
    ],
    "object-literal-key-quotes": false,
    "ban-types": false
  },
  "linterOptions": {
      "exclude": [
        "./node_modules/**"
      ]
  }
}